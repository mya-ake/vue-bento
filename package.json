{
  "name": "musubii-for-vue",
  "version": "1.0.0",
  "description": "A Component Library for Vue.js.",
  "main": "dist/musibii.common.js",
  "author": "Sho Yamane",
  "license": "MIT",
  "private": false,
  "module": "dist/musubii.esm.js",
  "unpkg": "dist/musubii.min.js",
  "style": "dist/style/index.css",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/musubii.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/musubii.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/musubii.min.js",
    "build:style": "gulp build:sass --gulpfile build/gulpfile.js",
    "lint": "vue-cli-service lint",
    "lint:nofix": "vue-cli-service lint --no-fix ",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.3",
    "@vue/cli-plugin-unit-jest": "^3.1.0",
    "@vue/cli-service": "^3.1.1",
    "@vue/cli-service-global": "^3.1.1",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "gulp": "^3.9.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.9.4",
    "node-sass-package-importer": "^5.2.0",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-vue": "^4.3.2",
    "sass-dashi": "^0.3.0",
    "sass-loader": "^7.1.0",
    "shitajicss": "^4.6.0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "dependencies": {
    "musubii": "^6.5.0"
  }
}
